<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_fragment_inicio">

    <fragment
        android:id="@+id/navigation_fragment_inicio"
        android:name="com.parroquiasanleandro.FragmentInicio"
        android:label="Inicio"
        tools:layout="@layout/fragment_inicio" />

    <fragment
        android:id="@+id/navigation_fragment_avisos"
        android:name="com.parroquiasanleandro.FragmentAvisosParroquiales"
        android:label="Avisos"
        tools:layout="@layout/fragment_avisos_parroquiales" />

    <fragment
        android:id="@+id/navigation_fragment_menu"
        android:name="com.parroquiasanleandro.FragmentPerfil"
        android:label="Menu"
        tools:layout="@layout/fragment_perfil" />

    <fragment
        android:id="@+id/navigation_fragment_informacion"
        android:name="com.parroquiasanleandro.FragmentInformacion"
        android:label="Informacion"
        tools:layout="@layout/fragment_informacion" />

    <fragment
        android:id="@+id/navigation_fragment_perfil"
        android:name="com.parroquiasanleandro.FragmentPerfil"
        android:label="Perfil"
        tools:layout="@layout/fragment_perfil" />
</navigation>